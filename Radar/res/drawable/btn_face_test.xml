<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="@dimen/test_normal_button_radius" />
            <gradient android:angle="270" android:endColor="@color/test_button_bright_end" android:startColor="@color/test_button_bright_start" />
        </shape>
    </item>
    <item>
        <shape>
            <corners android:radius="@dimen/test_normal_button_radius" />
            <gradient android:angle="270" android:endColor="@color/test_button_dark_end" android:startColor="@color/test_button_dark_start" />
        </shape>
    </item>
</selector>
